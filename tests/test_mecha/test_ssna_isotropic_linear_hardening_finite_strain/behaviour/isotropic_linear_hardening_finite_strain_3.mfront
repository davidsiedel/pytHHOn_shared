@DSL Implicit;
@Behaviour FiniteStrainIsotropicLinearHardeningPlasticity2;
@Author Thomas Helfer;
@Date 09 / 03 / 2021;
@Description {
    An implicit implementation of a simple
}

@ModellingHypotheses{".+"};
@StrainMeasure Hencky;

@Algorithm NewtonRaphson;
@Epsilon 1.e-14;
@Theta 1;

@Brick StandardElastoViscoPlasticity{
  stress_potential : "Hooke" {young_modulus : 70e9, poisson_ratio : 0.34},
  inelastic_flow : "Plastic" {
      criterion : "Mises",
      isotropic_hardening : "Linear" {R0 : 300.e6, H : 10e9}
  }
};